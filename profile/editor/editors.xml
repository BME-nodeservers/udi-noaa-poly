<editors>
    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>
    <editor id="TEMPERATURE">
        <range uom="17" min="-50" max="150" step="1" prec="1" />
        <range uom="4" min="-50" max="100" step="1" prec="1" />
    </editor>
    <editor id="PERCENT">
        <range uom="22" min="0" max="100" prec="0" />
    </editor>
    <editor id="SPEED">
        <range uom="48" min="0" max="500" prec="0" />
        <range uom="49" min="0" max="500" prec="0" />
    </editor>
    <editor id="DEGREES">
        <range uom="76" min="0" max="360" prec="0" />
    </editor>
    <editor id="WEATHER">
        <range uom="25" min="0" max="269" nls="EN_WEATHER" />
    </editor>
    <editor id="PRESSURE">
        <range uom="23" min="25" max="33" prec="3" />
        <range uom="117" min="800" max="1100" prec="1" />
        <range uom="118" min="800" max="1100" prec="1" />
    </editor>
    <editor id="DISTANCE">
        <range uom="116" min="0" max="500" prec="2" />
        <range uom="83"  min="0" max="10000" prec="1" />
    </editor>
	<editor id="ALERT">
		<range uom="25" min="0" max="49" NLS="EN_ALERT" />
	</editor>
	<editor id="ALERT_STATUS">
		<range uom="25" min="0" max="5" NLS="EN_ALERT_STATUS" />
	</editor>
	<editor id="ALERT_TYPE">
		<range uom="25" min="0" max="5" NLS="EN_ALERT_TYPE" />
	</editor>
	<editor id="ALERT_SEVERITY">
		<range uom="25" min="0" max="5" NLS="EN_ALERT_SEVERITY" />
	</editor>
	<editor id="ALERT_CERTAINY">
		<range uom="25" min="0" max="5" NLS="EN_ALERT_CERTAINY" />
	</editor>
	<editor id="ALERT_URGENCY">
		<range uom="25" min="0" max="5" NLS="EN_ALERT_URGENCY" />
	</editor>
	<editor id="ALERT_CATEGORY">
		<range uom="25" min="0" max="12" NLS="EN_ALERT_CATEGORY" />
	</editor>

</editors>
